{
	"name": "columnpaste",
	"displayName": "Column Paste",
	"description": "Copy/Paste a column text",
	"version": "0.0.7",
	"license": "MIT",
	"publisher": "johnguo",
	"engines": {
		"vscode": "^1.44.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/john-guo/columnpaste.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.columnpaste",
		"onCommand:extension.columnpasteautofilled"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.columnpaste",
				"title": "Column Paste"
			},
			{
				"command": "extension.columnpasteautofilled",
				"title": "Column Paste Auto Filled"
			}
		],
		"keybindings": [
			{
				"command": "extension.columnpaste",
				"key": "ctrl+alt+v"
			},
			{
				"command": "extension.columnpasteautofilled",
				"key": "shift+ctrl+v"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.37",
		"@types/vscode": "^1.44.0",
		"glob": "^7.1.5",
		"mocha": "^9.1.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {}
}
